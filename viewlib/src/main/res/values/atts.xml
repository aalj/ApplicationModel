<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SmsButText">
        <!--默认显示的文字-->
        <attr name="default_text" format="string" />
        <attr name="default_text_color" format="color" />
        <!--发送成功以后显示文字  倒数的数字以 %s 代替-->
        <attr name="send_text" format="string" />
        <attr name="send_text_color" format="color" />
        <attr name="countdown" format="integer" />
    </declare-styleable>
</resources>